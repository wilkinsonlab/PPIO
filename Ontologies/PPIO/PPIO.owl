    <!-- http://purl.oclc.org/PPIO#Blight -->

    <owl:Class rdf:about="&PPIO;Blight">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&PPIO;modifies"/>
